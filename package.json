{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "Vanilla is a single page application starter template.",
  "main": "server.js",
  "scripts": {
    "start": "watch 'npm run build:dev' app & node server.js",
    "build": "rimraf ./dist && NODE_ENV=production webpack --config app/webpack/config.js",
    "build:dev": "rimraf ./dist && NODE_ENV=development webpack --config app/webpack/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekingunoncu/vanilla.git"
  },
  "keywords": [
    "vanilla",
    "single page application",
    "single page",
    "web app",
    "core js",
    "core javascript",
    "spa",
    "starter"
  ],
  "author": "Ekin Gün Öncü",
  "bugs": {
    "url": "https://github.com/ekingunoncu/vanilla/issues"
  },
  "homepage": "https://github.com/ekingunoncu/vanilla#readme",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.13.8",
    "axios": "^0.21.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "watch": "^1.0.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
